#shell
alias ...=../..
alias ....=../../..
alias .....=../../../..
alias ......=../../../../..
alias .......=../../../../../..
alias ls='ls --color=tty'

#docker
drmf() {
  docker rm -f $1
}
alias drm=drmf
alias dkill='docker rm -f $(docker ps -aq)'
alias dps='docker ps'

#git
alias gp='git push'
alias gst='git status'
alias grh='git reset --hard'
alias gup='git pull --rebase'
